#include <iostream>
using namespace std;

int main() {
    int arr[100], n;

    cout << "Enter the number of elements: ";
    cin >> n;

    cout << "Enter " << n << " elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    for (int i = 0; i < n; i++) {
        for (int b = i + 1; b < n; ) {   
            if (arr[i] == arr[b]) {
                
                for (int a = b; a < n - 1; a++) {  
                    arr[a] = arr[a + 1];
                }
                n--; 
            } else {
                b++; 
            }
        }
    }

    cout << "Array after removing all the duplicates:\n";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}

